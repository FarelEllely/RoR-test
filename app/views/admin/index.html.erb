<h1>Admin Panel</h1>

<h3>Users</h3>

<%= will_paginate @users %>

<ul>
  <% @users.each do |user| %>
    <li>
      <strong><%= user.email %></strong>
      <% unless current_user == user %>
        <%= link_to 'Send message', new_message_path(to: user.id), class: 'btn btn-default btn-sm' %>
      <% end %>
    </li>
  <% end %>
</ul>

<%= will_paginate @users %>
